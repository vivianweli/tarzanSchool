<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Start_controller">
   <children>
      <AnchorPane prefHeight="450.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="450.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/bg.png" />
               </image>
            </ImageView>
            <Button fx:id="start" layoutX="250.0" layoutY="180.0" mnemonicParsing="false" onAction="#OnStart" prefHeight="30.0" prefWidth="100.0" text="Start" textFill="#604a4a">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="character" layoutX="250.0" layoutY="230.0" mnemonicParsing="false" onAction="#OnCharacter" prefHeight="30.0" prefWidth="100.0" text="Character" textFill="#604a4a">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="help" layoutX="250.0" layoutY="280.0" mnemonicParsing="false" onAction="#OnHelp" prefHeight="30.0" prefWidth="100.0" text="Help" textFill="#604a4a">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="quit" layoutX="250.0" layoutY="330.0" mnemonicParsing="false" onAction="#OnQuit" prefHeight="30.0" prefWidth="100.0" text="Quit" textFill="#604a4a">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
